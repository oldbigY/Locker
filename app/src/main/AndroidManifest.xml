<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.locker.android.locker" >

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/iconlocker2"
        android:theme="@android:style/Theme.Light">
        <activity
            android:theme="@style/appTheme"
            android:name=".atys.AtyLogin"
            android:label="@string/app_name"
            android:icon="@drawable/iconlocker2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".atys.AtyRegister"
            android:label="Register"
            android:theme="@style/appTheme" />
        <activity android:name=".atys.AtyOperation"
            android:label="Operation"
            android:theme="@style/appTheme" />
        <activity android:name=".atys.AtySettings"
            android:theme="@style/CustomTitleBar"
            android:label="@string/item_settings"/>
        <activity android:name=".atys.AtyChangelockpassword"
            android:theme="@style/CustomTitleBar"
            android:label="@string/item_settings"/>
        <activity android:name=".atys.AtyChangeuserpassword"
            android:theme="@style/CustomTitleBar"
            android:label="@string/item_settings"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
